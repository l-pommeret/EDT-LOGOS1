{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "id": {
                "description": "The unique identifier for the calendar",
                "type": "string"
            },
            "url": {
                "description": "The url to the calendar.",
                "type": "string",
                "format": "uri"
            },
            "label": {
                "description": "The label of the calendar.",
                "type": "string"
            },
            "parcours": {
                "description": "The parcours of the calendar.",
                "type": "string"
            },
            "year": {
                "description": "The year of the calendar.",
                "type": "string"
            }
        },
        "required": [
            "id",
            "url",
            "label",
            "parcours",
            "year"
        ]
    }
}
